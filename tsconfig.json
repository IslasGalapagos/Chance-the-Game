{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "lib": ["es2015", "es2016", "es2017", "dom"],
    "jsx": "react",
    "baseUrl": ".",
    "moduleResolution": "node",
    "removeComments": true,
    "typeRoots": ["node_modules/@types"],
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "pretty": true,
    "esModuleInterop": true
  }
}
